programa {
  funcao inicio() {
    inteiro contador = 1
    inteiro fim = 0
    escreva ("informe a quantidade de vezes a mensagem ira aparecer:")
    leia (fim)
    escreva("\n")
    enquanto (contador <= fim){
        escreva ("estou estudando programaçao:\n") 
        contador = contador + 1
    }
   // escreva("saiu do laço")
  }
}
